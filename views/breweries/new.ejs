<%- include('../partials/header') %>

<form id="new-form" action="/breweries" method="POST">
    <div class "input-group">
  <label>Brewery: </label>
    <input type="text" name="name" placeholder="Brewery Name"/>
  <label>Type of Brewery: </label>
    <input type="text" name="brewery_type" placeholder="Brewery Type"/>
  <label>Street Address: </label>
    <input type="text" name="street" placeholder="123 Main St"/>
  <label>City: </label>
    <input type="text" name="city" placeholder="City"/>
  <label>State: </label>
    <input type="text" name="state" placeholder="State"/>
  <label>Zip: </label>
    <input type="text" name="postal_code" placeholder="zipcode"/>
  <label>Country: </label>
    <input type="text" name="country" placeholder="country"/>
  <label>Latitude: </label>
    <input type="text" name="latitude" placeholder="38.44174"/>
  <label>Longitude: </label>
    <input type="text" name="longitude" placeholder="-122.71156"/>
  <label>Phone: </label>
    <input type="text" name="phone" placeholder="5558675309"/>
  <label>Website: </label>
    <input type="text" name="website_url" placeholder="www.brewery.beer"/>
    <br><br>
    <button class="btn btn-success" type="submit">BEER</button>
    </div>
</form>

<%- include('../partials/footer') %>
